/*头部*/
.margin{width:1200px;margin:0 auto;}
.integra{background:url(../img/integra.png) no-repeat;}
.integra1{background:url(../img/integra1.png) no-repeat;}


#top{height:30px;background:#f6f6f6;font:12px/30px "";border-bottom:1px solid #dbdbdb;}
#top .left{float:left;width:444px;display:flex;justify-content:space-around;}
#top .left a:hover{color:#f00;text-decoration:underline;}
#top .right{float:right;width:600px;display:flex;justify-content:space-around;}
#top .right li{float:left;display:flex;align-items:center;padding:0 10px}
#top .right li:nth-child(1){position: relative;box-sizing:border-box;width:100px;border-left:1px solid #f6f6f6;border-right:1px solid #f6f6f6;}
#top .right li:nth-child(1):hover{background:#fff;border-left:1px solid #ccc;border-right:1px solid #ccc;width:100px;}
#top .right li:nth-child(1) .over1{display:flex;align-items:center;position:absolute;}
#top .right li:nth-child(1) .over1 i:nth-child(1){background-position:0 0;width:16px;height:13px;}
#top .right li:nth-child(1) .over1 a{padding:0 5px;}
#top .right li:nth-child(1):hover .over1 a{color:#f00;text-decoration:underline;}
#top .right li:nth-child(1) .over1 i:nth-child(3){background-position:-16px 0;width:6px;height:3px;}
#top .right li:nth-child(1) .hide1{display:none;background:#fff;border:1px solid #e2e2e2;border-top:0;position:absolute;top: 31px;left: 0;width: 270px;padding:8px 0;z-index:4;}
#top .right li:nth-child(1):hover .hide1{display: block;left:-1px;}
#top .right li:nth-child(1) .hide1 p{overflow:hidden;padding:0 10px;}
#top .right li:nth-child(1) .hide1 p span{float:left;color: #f00;}
#top .right li:nth-child(1) .hide1 p a{float:right;}
#top .right li:nth-child(1) .hide1 .fox{display: flex;}
#top .right li:nth-child(1) .hide1 dl{flex: 1;padding:0 10px;}
#top .right li:nth-child(1) .hide1 dl:nth-child(1){border-right:1PX dashed #ccc;}
#top .right li:nth-child(1) .hide1 dl dd{border-bottom:1px dashed #ccc;width: 104px;float:left;line-height: 30px;}
#top .right li:nth-child(1) .hide1 a:hover{color:#f00;}
#top .right li:nth-child(2) i{background-position:-22px 0;width:22px;height:13px;margin-right:5px;}
#top .right li span:hover{color:#f00;}
#top .right li:nth-child(3){position: relative;box-sizing:border-box;width:100px;border-left:1px solid #f6f6f6;border-right:1px solid #f6f6f6;}
#top .right li:nth-child(3):hover{background:#fff;border-left:1px solid #ccc;border-right:1px solid #ccc;}
#top .right li:nth-child(3) .over3{display:flex;align-items:center;position:absolute;}
#top .right li:nth-child(3) .over3 i:nth-child(1){background-position:-44px 0;width:10px;height:15px;}
#top .right li:nth-child(3) .over3 a{padding:0 5px;}
#top .right li:nth-child(3):hover .over3 a{color:#f00;text-decoration:underline;}
#top .right li:nth-child(3) .over3 i:nth-child(3){background-position:-16px 0;width:6px;height:3px;}
#top .right li:nth-child(3) .hide3{display:none;width:250px;border:1px solid #e2e2e2; border-top:0;background:#fff;position:absolute;left:0;top: 31px;padding: 8px 0;z-index: 3;}

#top .right li:nth-child(3):hover .hide3{display: block;left:-1px;}
#top .right li:nth-child(3) .hide3 dd{height:100px;border-bottom:1px dashed #ccc;}
#top .right li:nth-child(3) .hide3 dd img{float:left;width:76px;height:76px;margin:10px;}
#top .right li:nth-child(3) .hide3 dd u{float:left;margin-top:10px;}
#top .right li:nth-child(3) .hide3 dd u p{font:12px/18px "";color:#f00;}
#top .right li:nth-child(3) .hide3 dd u i:nth-child(3){display: inline-block;background-position:-51px -75px;width:45px;height:45px;margin-right:10px;}
#top .right li:nth-child(3) .hide3 dd u i:nth-child(4){display: inline-block;background-position:-51px -125px;width:45px;height:45px;}
#top .right li:nth-child(6){position: relative;box-sizing:border-box;border-left:1px solid #f6f6f6;border-right:1px solid #f6f6f6;}
#top .right li:nth-child(6):hover{background:#fff;border-left:1px solid #ccc;border-right:1px solid #ccc;}
#top .right li:nth-child(6) .over6{display:flex;align-items:center;position:absolute;}
