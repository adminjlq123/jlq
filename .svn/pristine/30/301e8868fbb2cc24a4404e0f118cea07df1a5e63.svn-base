/*banner轮播图加登陆*/
#banner{height:465px;border-bottom:2px solid #eee;}
#banner .lunbo{float:left;width:770px;height:465px;margin-left:230px;position: relative;overflow:hidden;}
#banner .lunbo .imgbox{width:770px;height:465px;}
#banner .lunbo .imgbox img{width:770px;height:465px;position:absolute;left:770px;}
#banner .lunbo .imgbox img:nth-child(1){left:0;}
#banner .btn input{width:50px;height:50px;background: #eee;border: 0;position: absolute; z-index:2;top:200px;opacity: 0.5}
#banner .btn .left{left:30px;}
#banner .btn .right{right:30px;}
/*登陆*/
#banner .rig{float:right;width:200px;}
#banner .rig .top{padding-top:10px;text-align:center;}
#banner .rig .top .hi{overflow:hidden;}
#banner .rig .top .hi i{float:left;margin:0 10px 5px 30px;background-position:-54px -27px;width:48px;height:48px;}
#banner .rig .top .hi p{float:left;margin-top:5px;font:12px/20px "";}
#banner .rig .top .new{padding:5px 0;}
#banner .rig .top .new a{display:inline-block;margin:5px;padding:5px 15px;background:#fff; color:#D51938;border-radius:15px;box-shadow:0px 0px 30px rgba(179, 179, 179, 0.51);}
#banner .rig .top .new a:hover{background:#000;color:#fff;}
#banner .rig .cen{padding:0 10px;}
#banner .rig .cen p{overflow:hidden;text-align:center;}
#banner .rig .cen p a{float:left;width:63px;height:30px;}
#banner .rig .cen p a span{padding:5px 0;font:12px/26px "";}
#banner .rig .cen p u{float:left;margin-top:6px;}
#banner .rig .cen .bott{border-bottom:1px solid #ccc;}
#banner .rig .cen .bott ul{padding-bottom:10px;}
#banner .rig .cen .bott ul li{width:180px;height:15px;padding:5px 0;overflow:hidden;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
#banner .rig .cen .bott ul li:nth-child(1) a{color:#d11e31;}
#banner .rig .cen .bott ul li:nth-child(2) a{color:#d11e31;}
#banner .rig .cen .bott ul li:hover a{color:#d11e31;}
#banner .rig .bot{width:100%;}
#banner .rig .bot a{width:50%;float:left;padding:10px 0;text-align:center;}
#banner .rig .bot a:nth-child(1) i{display:inline-block;background-position:-22px -16px;width:25px;height:25px;}
#banner .rig .bot a:nth-child(2) i{display:inline-block;background-position:-22px -41px;width:25px;height:25px;}
#banner .rig .bot a:nth-child(3) i{display:inline-block;background-position:-22px -66px;width:25px;height:25px;}
#banner .rig .bot a:nth-child(4) i{display:inline-block;background-position:-22px -91px;width:25px;height:25px;}
#banner .rig .bot a span{display:block;padding-top:5px;font:12px/14px "";color:#ffa243;}
/*运动选项卡*/
#spt{margin-top:20px;height:488px}
#spt .left{width:260px;float:left;}
#spt .left .tit{display:block;width:260px;}
#spt .left .tit img{width:260px;height:50px;}
#spt .left .zi{border:1px solid #dbdbdb;border-bottom:0;}
#spt .left .zi h3{font:18px/2 "";padding-top:10px;margin-left:20px;}
#spt .left .nav{width:258px;border-style: solid;border-width:0 1px 1px;border-color: #dbdbdb;}
#spt .left .nav ul{padding:0 20px;overflow:hidden;}
#spt .left .nav ul li{float:left;}
#spt .left .nav ul li a{display:block;width:100px;height:40px;padding:5px 4px;}
#spt .left .nav ul li a img{width:100px;height:40px;} 
#spt .left .nav p{font:14px/16px "";color:#828282;text-align:right;padding-right:15px;margin:12px 0 ;}
#spt .mid{width:680px;float:left;}
#spt .mid .tit{width:680px;margin-top:14px;}
#spt .mid .tit li{width: 170px;color:#a4d203;float:left;font:16px/36px "";position: relative;text-align: center;color:#848282;cursor:pointer;border-bottom:2px solid #a7a7a7;}
#spt .mid .tit li.dangqian{border-bottom: 2px solid #a4d203;}
#spt .mid .tit li.dangqian b{width:1px;border-bottom:8px solid #a4d203;border-right:8px solid  #fff;border-left:8px solid #fff;;position: absolute;left:80px;top:28px;}
#spt .mid .goods{width:680px;position: relative;}
#spt .mid .goods .go{position:absolute;top:38px;}
#spt .mid .goods .go li{position:relative;width:139px;height:212px;float:left;border-right:1px solid #dbdbdb;border-bottom:1px solid #dbdbdb;padding:0 15px 5px;overflow:hidden;background:#fff;}
#spt .mid .goods .go li .to{padding:5px 0;}
#spt .mid .goods .go li .to a{display:block;text-align:center;}
#spt .mid .goods .go li .mi{height:36px;line-height:18px;overflow:hidden;}
#spt .mid .goods .go li .bo{}
#spt .mid .goods .go li .bo u{}
#spt .mid .goods .go li .bo s{color:#e74208;font:700 14px/16px "";}
#spt .right{width:260px;height:488px;float:left;}
#spt .right .top{border:1px solid #dbdbdb;border-left:0;margin-top:50px;padding-bottom:2px;}
#spt .right .bot{border-right:1px solid #dbdbdb;border-bottom:1px solid #dbdbdb;}
#spt .right a{display: block;}
