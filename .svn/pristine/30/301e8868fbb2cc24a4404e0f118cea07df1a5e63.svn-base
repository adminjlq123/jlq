#top .right li:nth-child(6) .hide6{display:none;background:#fff;border:1px solid #e2e2e2; border-top:0;position: absolute;right: 0;top:31px;width: 150px;padding: 8px 10px;z-index: 3;}
#top .right li:nth-child(6):hover .hide6{display: block;right:-1px;}

header{height:110px;}
header .logo{float:left;margin-right:20px;}
header .cen{float:left;margin-top:30px;}
header .inp{height:40px;}
